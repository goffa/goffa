                file:///usr/share/openbox/menu.xsd">

<menu id="tools-menu" label="Tools">
  <item label="console">
    <action name="Execute"><execute>aterm -title console +sb -fg gray90 -bg black -sl 1024 -tr -sh 25 -fn -Misc-Fixed-Bold-R-Normal--13-120-75-75-C-70-ISO8859-1</execute></action>
  </item>
  <item label="xterm">
     <action name="Execute"><execute>xterm</execute></action>
</item>
  <item label="file manager">
    <action name="Execute"><execute>rox</execute></action>
  </item>
  <item label="text editor">
    <action name="Execute"><execute>leafpad</execute></action>
  </item>
  <item label="calculator">
    <action name="Execute"><execute>galculator</execute></action>
  </item>
  <item label="font sampler">
    <action name="Execute"><execute>gfontsampler</execute></action>
  </item>
  <item label="package organizer">
    <action name="Execute"><execute>sudo gpaco</execute></action>
  </item>
</menu>

<menu id="net-menu" label="Internet">
